{
    "environment": "QbertNoFrameskip-v4",
    "random_seed": 0,
    "number_generations": 100,
    "use_worker_processes": true,
    "optimizer": {
        "type": "MU_ES",
        "hof_size": 20,
        "checkpoint_frequency": 10,
        "initial_gene_range": 2,
        "tournsize": 100,
        "mu": 20,
        "extra_from_hof": 0,
        "lambda_": 1000,
        "mutpb": 0.8,
        "efficiency_weight": 0.1
    },
    "novelty": {
        "behavioral_interval": 1,
        "behavioral_max_length": 100,
        "behavior_from_observation": true,
        "distance": "NCD",
        "max_recorded_behaviors": 30,
        "recorded_behaviors_per_generation": 3,
        "novelty_nearest_k": 5,
        "novelty_weight": 2.0
    },
    "brain": {
        "type": "CTRNN",
        "number_neurons": 40,
        "use_bias": true,
        "neuron_activation": "tanh",
        "neuron_activation_inplace": false,
        "delta_t": 1.0,
        "normalize_input": false,
        "normalize_input_target": 3,
        "optimize_state_boundaries": "global",
        "clipping_range_max": 1.0,
        "clipping_range_min": -1.0,
        "optimize_y0": true,
        "set_principle_diagonal_elements_of_W_negative": true,
        "v_mask": "logarithmic",
        "v_mask_param": 16,
        "w_mask": "logarithmic",
        "w_mask_param": 4,
        "t_mask": "logarithmic",
        "t_mask_param": 4,
        "parameter_perturbations": 0.001
    },
    "episode_runner": {
        "keep_env_seed_fixed_during_generation": true,
        "number_fitness_runs": 1,
        "max_steps_per_run": 0,
        "max_steps_penalty": 0,
        "reuse_env": true
    }
}