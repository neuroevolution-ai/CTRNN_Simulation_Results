{
    "environment": "BipedalWalker-v3",
    "random_seed": 0,
    "number_generations": 150,
    "use_worker_processes": true,
    "optimizer": {
        "type": "MU_ES",
        "hof_size": 10,
        "extra_from_hof": 2,
        "checkpoint_frequency": 10,
        "initial_gene_range": 2,
        "tournsize": 5,
        "mu": 50,
        "lambda_": 150,
        "mutpb": 0.8,
        "efficiency_weight": 0.05
    },
    "novelty": {
        "behavior_from_observation": true,
        "distance": "NCD",
        "max_recorded_behaviors": 50,
        "recorded_behaviors_per_generation": 2,
        "novelty_nearest_k": 5,
        "novelty_weight": 0.5,
        "behavioral_interval": 10,
        "behavioral_max_length": 50
    },
    "brain": {
        "type": "CTRNN",
        "number_neurons": 10,
        "delta_t": 0.5,
        "normalize_input": false,
        "normalize_input_target": 5,
        "optimize_state_boundaries": "global",
        "clipping_range_max": 1.0,
        "clipping_range_min": -1.0,
        "optimize_y0": true,
        "set_principle_diagonal_elements_of_W_negative": true,
        "v_mask": "learned",
        "v_mask_param": 90,
        "w_mask": "learned",
        "w_mask_param": 80,
        "t_mask": "learned",
        "t_mask_param": 90,
        "parameter_perturbations": 0.001,
        "neuron_activation": "relu",
        "neuron_activation_inplace": false,
        "use_bias": false
    },
    "episode_runner": {
        "keep_env_seed_fixed_during_generation": true,
        "number_fitness_runs": 1,
        "reuse_env": true,
        "max_steps_per_run": 0,
        "max_steps_penalty": 0
    }
}