{
  "environment": "LunarLander-v2",
  "random_seed": 0,
  "number_generations": 500,
  "use_worker_processes": true,
  "optimizer": {
    "type": "CMA_ES",
    "population_size": 50,
    "sigma": 1.0,
    "checkpoint_frequency": 0,
    "hof_size": 5,
    "efficiency_weight": 0,
    "fix_seed_for_generation": false,
    "mu": 0
  },
  "brain": {
    "type": "CTRNN",
    "number_neurons": 10,
    "neuron_activation": "relu",
    "neuron_activation_inplace": true,
    "delta_t": 0.5,
    "normalize_input": false,
    "normalize_input_target": 5,
    "optimize_state_boundaries": "global",
    "clipping_range_max": 1.0,
    "clipping_range_min": -1.0,
    "optimize_y0": true,
    "set_principle_diagonal_elements_of_W_negative": true,
    "v_mask": "logarithmic",
    "v_mask_param": 4,
    "w_mask": "logarithmic",
    "w_mask_param": 4,
    "t_mask": "logarithmic",
    "t_mask_param": 4,
    "parameter_perturbations": 0.001,
    "use_bias": false
  },
  "episode_runner": {
    "keep_env_seed_fixed_during_generation": true,
    "number_fitness_runs": 3,
    "max_steps_per_run": 20000,
    "reuse_env": true,
    "max_steps_penalty": 0,
    "use_autoencoder": false
  }
}
