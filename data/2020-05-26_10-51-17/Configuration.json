{
  "environment": "BipedalWalker-v3",
  "random_seed": 123,
  "number_generations": 500,
  "optimizer": {
    "type": "CMA_ES",
    "population_size": 150,
    "sigma": 1.0,
    "checkpoint_frequency": 20
  },
  "brain": {
    "type": "CTRNN",
    "number_neurons": 30,
    "delta_t": 0.05,
    "normalize_input": false,
    "normalize_input_target": 2,
    "optimize_state_boundaries": "legacy",
    "clipping_range_max": 0.1,
    "clipping_range_min": -0.1,
    "optimize_y0": true,
    "set_principle_diagonal_elements_of_W_negative": true,
    "w_mask": "logarithmic",
    "w_mask_param": 4,
    "v_mask": "logarithmic",
    "v_mask_param": 4,
    "t_mask": "logarithmic",
    "t_mask_param": 4,
    "parameter_perturbations": 0.001
  },
  "episode_runner": {
    "keep_env_seed_fixed_during_generation": false,
    "number_fitness_runs": 1,
    "reuse_env": false
  }
}