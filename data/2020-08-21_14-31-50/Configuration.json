{
    "environment": "ReacherMemory-v0",
    "random_seed": 0,
    "number_generations": 2000,
    "use_worker_processes": true,
    "optimizer": {
        "type": "CMA_ES",
        "population_size": 500,
        "sigma": 1.0,
        "checkpoint_frequency": 10,
        "hof_size": 10,
        "mu": 50,
        "efficiency_weight": 0,
        "fix_seed_for_generation": true
    },
    "brain": {
        "type": "CTRNN",
        "number_neurons": 8,
        "neuron_activation": "tanh",
        "neuron_activation_inplace": false,
        "use_bias": true,
        "delta_t": 0.05,
        "normalize_input": false,
        "normalize_input_target": 2,
        "optimize_state_boundaries": "fixed",
        "clipping_range_max": 1.0,
        "clipping_range_min": -1.0,
        "optimize_y0": false,
        "set_principle_diagonal_elements_of_W_negative": false,
        "parameter_perturbations": 0.0,
        "w_mask": "dense",
        "w_mask_param": 4,
        "v_mask": "dense",
        "v_mask_param": 4,
        "t_mask": "dense",
        "t_mask_param": 4
    },
    "novelty": {
        "behavioral_interval": 10,
        "behavioral_max_length": 9,
        "behavior_from_observation": true,
        "distance": "euclid",
        "max_recorded_behaviors": 100,
        "recorded_behaviors_per_generation": 2,
        "novelty_nearest_k": 25,
        "novelty_weight": 0.2
    },
    "episode_runner": {
        "number_fitness_runs": 10,
        "reuse_env": true,
        "max_steps_per_run": 0,
        "max_steps_penalty": 0,
        "keep_env_seed_fixed_during_generation": true,
        "environment_attributes": {
            "observation_frames": 20,
            "memory_frames": 20,
            "action_frames": 50
        }
    }
}