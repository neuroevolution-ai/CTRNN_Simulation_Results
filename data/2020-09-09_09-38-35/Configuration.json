{
    "environment": "QbertNoFrameskip-v4",
    "random_seed": -1,
    "number_generations": 140,
    "use_worker_processes": true,
    "optimizer": {
        "type": "MU_ES",
        "hof_size": 10,
        "checkpoint_frequency": 10,
        "initial_gene_range": 2,
        "tournsize": 0,
        "mu": 500,
        "extra_from_hof": 1,
        "lambda_": 3000,
        "mutpb": 0.8,
        "efficiency_weight": 0.2,
        "fix_seed_for_generation": true,
        "strategy_parameter_per_gene": false
    },
    "brain": {
        "type": "CNN_CTRNN",
        "normalize_input": false,
        "normalize_input_target": 0.0,
        "use_bias": false,
        "cnn_conf": {
            "type": "CNN",
            "normalize_input": false,
            "normalize_input_target": 0.0,
            "use_bias": false
        },
        "ctrnn_conf": {
            "type": "CTRNN",
            "number_neurons": 20,
            "neuron_activation": "relu",
            "neuron_activation_inplace": false,
            "use_bias": true,
            "delta_t": 0.1,
            "normalize_input": false,
            "normalize_input_target": 2,
            "optimize_state_boundaries": "global",
            "clipping_range_max": 1.0,
            "clipping_range_min": -1.0,
            "optimize_y0": false,
            "set_principle_diagonal_elements_of_W_negative": false,
            "parameter_perturbations": 0.0,
            "w_mask": "logarithmic",
            "w_mask_param": 2,
            "v_mask": "logarithmic",
            "v_mask_param": 2,
            "t_mask": "logarithmic",
            "t_mask_param": 2
        }
    },
    "novelty": {
        "behavioral_interval": 10,
        "behavioral_max_length": 20,
        "behavior_source": "state",
        "distance": "NCD",
        "max_recorded_behaviors": 100,
        "recorded_behaviors_per_generation": 2,
        "novelty_nearest_k": 80,
        "novelty_weight": 1.0
    },
    "episode_runner": {
        "number_fitness_runs": 2,
        "reuse_env": true,
        "max_steps_per_run": 1500,
        "max_steps_penalty": 0,
        "keep_env_seed_fixed_during_generation": true,
        "use_autoencoder": false
    }
}