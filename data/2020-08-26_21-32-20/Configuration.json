{
    "environment": "ReacherMemoryDynamic-v0",
    "random_seed": 0,
    "number_generations": 6660,
    "use_worker_processes": true,
    "optimizer": {
        "type": "MU_ES",
        "hof_size": 10,
        "checkpoint_frequency": 10,
        "initial_gene_range": 2,
        "tournsize": 5,
        "mu": 300,
        "extra_from_hof": 0,
        "lambda_": 1000,
        "mutpb": 0.8,
        "efficiency_weight": 0.0,
        "fix_seed_for_generation": true
    },
    "brain": {
        "type": "CTRNN",
        "number_neurons": 8,
        "neuron_activation": "tanh",
        "neuron_activation_inplace": false,
        "use_bias": false,
        "delta_t": 0.05,
        "normalize_input": false,
        "normalize_input_target": 2,
        "optimize_state_boundaries": "global",
        "clipping_range_max": 1.0,
        "clipping_range_min": -1.0,
        "optimize_y0": false,
        "set_principle_diagonal_elements_of_W_negative": false,
        "parameter_perturbations": 0.0,
        "w_mask": "dense",
        "w_mask_param": 4,
        "v_mask": "dense",
        "v_mask_param": 4,
        "t_mask": "dense",
        "t_mask_param": 4
    },
    "novelty": {
        "behavioral_interval": 9,
        "behavioral_max_length": 10,
        "behavior_source": "brain",
        "distance": "euclid",
        "max_recorded_behaviors": 200,
        "recorded_behaviors_per_generation": 2,
        "novelty_nearest_k": 25,
        "novelty_weight": 0.3
    },
    "episode_runner": {
        "number_fitness_runs": 5,
        "reuse_env": true,
        "max_steps_per_run": 0,
        "max_steps_penalty": 0,
        "keep_env_seed_fixed_during_generation": true,
        "use_autoencoder": false,
        "environment_attributes": {
            "observation_frames": 20,
            "memory_frames": 1,
            "action_frames": 50
        }
    }
}
